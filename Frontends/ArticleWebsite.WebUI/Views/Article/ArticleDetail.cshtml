
@{
    ViewData["Title"] = "ArticleDetail";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<section class="ftco-section ftco-degree-bg">
    <div class="container">
        <div class="row">

            @await Component.InvokeAsync("_ArticleDetailsMainComponentPartial",new {id=ViewBag.articleId})
            
            <div class="col-md-4 sidebar ftco-animate">
                
                @await Component.InvokeAsync("_ArticleDetailSideBarSearchBoxComponentPartial")
                

                @await Component.InvokeAsync("_ArticleDetailsDepartmentViewComponentPartial")


                @await Component.InvokeAsync("_ArticleDetailsRecentArticlesComponentPartial")


                @await Component.InvokeAsync("_ArticleDetailsTagCloudComponentPartial")

                
            </div>

        </div>
    </div>
</section>

